<%- include('partials/head.ejs') %>
	<body class="story">

	<a class="edit-story-btn" href="/create-story/<%- id %>" role="button">Bewerk dit verhaal</a>

	<% for(var i = 0; i < Object.keys(dataFirstQuery).length; i++) { %>
		<section class="year">
			<h1><%- Object.keys(dataFirstQuery)[i] %></h1>
			<% var objectYears = dataFirstQuery[Object.keys(dataFirstQuery)[i]]; %>
			<% for(var j = 0; j < Object.keys(objectYears).length; j++) { %>
				<section class="chapter">
					<h2>We zijn in jaar <%- Object.keys(dataFirstQuery)[i]%> en we bevinden ons bij <%- Object.keys(objectYears)[j] %> </h2>
					<div>
						<% var objectChapters = objectYears[Object.keys(objectYears)[j]] %>
						<% for(var k = 0; k < Object.keys(objectChapters).length; k++) { %>
							<div class="image">
								<label>
									<div>
										<section></section>
										<img src="<%- Object.values(objectChapters)[k].img.value %>" alt="<%- Object.values(objectChapters)[k].title.value %>"/>
										<p><%- Object.values(objectChapters)[k].title.value %></p>
									</div>
								</label>
							</div>
						<% } %>
					</div>
				</section>
			<% } %>
		</section>
	<% } %>

	<script>
		window.data = <%- JSON.stringify(dataFirstQuery || null) %>;
		console.log(window.data);
	</script>

<%- include('partials/foot.ejs') %>
